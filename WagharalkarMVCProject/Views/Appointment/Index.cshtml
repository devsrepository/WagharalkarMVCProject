
@{
    ViewBag.Title = "Index";
}
<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Content/Appointment.js"></script>
<h2>Index</h2>
<h1>Welcome to Appointment Page !!</h1>
<input type="text" id="txtName" placeholder="Please enter the Name" />
<br />
<input type="text" id="txtEmail" placeholder="Please enter the Email" />
<br />
<select id="ddlCity">
    <option>Nagpur</option>
    <option>Pune</option>
    <option>Mumbai</option>
    <option>Aurangabad</option>
</select>
<br />
<input type="text" id="txtMobileNo" placeholder="Please enter the MobileNo" />
<br />
<input type="text" id="txtAppointmentDate" placeholder="Please enter the AppointmentDate" />
<br />
<input type="text" id="txtGender" placeholder="Please enter the Gender" />
<br />
<input type="text" id="txtMessage" placeholder="Please enter the Message" />
<br />
<input type="text" id="txtCreateDate" placeholder="Please enter the CreateDate" />
<br />
<input type="text" id="txtUpdateDate" placeholder="Please enter the UpdateDate" />
<br />
<input type="text" id="txtCreatedby" placeholder="Please enter the id" />
<br />
<input type="text" id="txtUpdatedby" placeholder="Please enter the id" />
<br />
<input type="submit" id="btnSubmit" value="Submit" onclick="saveAppointment()" />

